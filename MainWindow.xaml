<Window x:Class="oop.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:oop"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TabControl>
            <TabItem Header="主页">
                <Frame NavigationUIVisibility="Hidden"  Name="Home"></Frame>
            </TabItem>
            <TabItem Header="下载">
                <Frame NavigationUIVisibility="Hidden"></Frame>
            </TabItem>
            <TabItem Header="设置">
                <Frame NavigationUIVisibility="Hidden" Name="Setting" ></Frame>
            </TabItem>
        </TabControl>
        
    </Grid>
</Window>
